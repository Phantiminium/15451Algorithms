#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

typedef int bool;
#define true 1
#define false 0

struct op {
  char type;
  int i;
  int j;
  int d;
};

struct node {
  int updates;
  int rangeMin;
  bool valid;
};

int SuperCeiling(int n) {
  int p;
  //int count = 0;
  for (p = 1; p < n; p = 2 * p);
  return p;
}

int max (int a, int b) {return (a > b) ? a : b;}
int min (int a, int b) {return (a < b) ? a : b;}
int Parent(int i) {return i / 2;}
int LeftChild(int i) {return 2 * i;}
int RightChild(int i) {return 2 * i + 1;}

int glue(int a, int b) {
  return min(a, b);
}

int identity = INT_MAX;
int n, N;
struct node **A;
