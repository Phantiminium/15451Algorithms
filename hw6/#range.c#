void printop(struct op **ops, int i) {
  printf("%c %d %d ", ops[i]->type, ops[i]->i, ops[i]->j);
  if (ops[i]->type == 'i') {
    printf("%d", ops[i]->d);
  }
  printf("\n");
}
/*
